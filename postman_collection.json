{
	"info": {
		"_postman_id": "swagger-express-node-postgres-aws",
		"name": "Swagger Express Node Postgres AWS API",
		"description": "Complete API collection for Swagger Express Node Postgres AWS backend",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
